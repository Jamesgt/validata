{
  "coverageReporters": [
    "cobertura",
    "lcov",
    "text"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "testEnvironment": "node",
  "testRegex": "/(test|src)/.*\\.(test)\\.(tsx?)$",
  "testEnvironmentOptions": {
    "url": "http://localhost"
  },
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  }
}
